@model IEnumerable<GamingGroove.Models.PublicacaoModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>




<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.comunidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.textoPublicacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.midiaPublicacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataPublicacao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.usuario.usuarioId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.comunidade.comunidadeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.textoPublicacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.midiaPublicacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataPublicacao)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.publicacaoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.publicacaoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.publicacaoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
